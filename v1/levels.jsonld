{
  "@context": "https://ontology.radical.world/v1/context.jsonld",
  "@graph": [
    {
      "@id": "rad:LevelScheme",
      "type": "ConceptScheme",
      "name": "Levels of Organization",
      "description": "A controlled vocabulary of organizational scales (bio, social, ecological, and cosmic)."
    },

    {
      "@id": "https://ontology.radical.world/level/subatomic",
      "type": "LevelOfOrganization",
      "name": "Subatomic",
      "alias": ["Quantum Scale","Particle Level"],
      "description": "Quarks, leptons, gauge bosons; quantum interactions.",
      "broader": ["https://ontology.radical.world/level/atomic"],
      "narrower": [],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/atomic",
      "type": "LevelOfOrganization",
      "name": "Atomic",
      "alias": ["Element Level"],
      "description": "Atoms as units of chemical elements; orbitals, ionization, bonding capacity.",
      "broader": ["https://ontology.radical.world/level/molecular"],
      "narrower": ["https://ontology.radical.world/level/subatomic"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/molecular",
      "type": "LevelOfOrganization",
      "name": "Molecular",
      "alias": ["Chemistry Level","Biomolecular"],
      "description": "Molecules and macromolecules (water, proteins, DNA/RNA); reactions and folding.",
      "broader": ["https://ontology.radical.world/level/organelle"],
      "narrower": ["https://ontology.radical.world/level/atomic"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/organelle",
      "type": "LevelOfOrganization",
      "name": "Organelle",
      "description": "Cellular compartments (nucleus, mitochondria, chloroplasts, ribosomes) performing localized functions.",
      "broader": ["https://ontology.radical.world/level/cellular"],
      "narrower": ["https://ontology.radical.world/level/molecular"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/cellular",
      "type": "LevelOfOrganization",
      "name": "Cellular",
      "alias": ["Cell"],
      "description": "Smallest living unit; membranes, metabolism, homeostasis, reproduction.",
      "broader": ["https://ontology.radical.world/level/tissue"],
      "narrower": ["https://ontology.radical.world/level/organelle"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/tissue",
      "type": "LevelOfOrganization",
      "name": "Tissue",
      "description": "Cooperating cells with similar structure and function (e.g., epithelial, muscle, vascular).",
      "broader": ["https://ontology.radical.world/level/organ"],
      "narrower": ["https://ontology.radical.world/level/cellular"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/organ",
      "type": "LevelOfOrganization",
      "name": "Organ",
      "description": "Anatomical units made of multiple tissues performing specialized functions (e.g., heart, leaf).",
      "broader": ["https://ontology.radical.world/level/organism"],
      "narrower": ["https://ontology.radical.world/level/tissue"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/organism",
      "type": "LevelOfOrganization",
      "name": "Organism",
      "alias": ["Individual"],
      "description": "An individual living system capable of independent function.",
      "broader": ["https://ontology.radical.world/level/population"],
      "narrower": ["https://ontology.radical.world/level/organ"],
      "inScheme": "rad:LevelScheme"
    },

    {
      "@id": "https://ontology.radical.world/level/population",
      "type": "LevelOfOrganization",
      "name": "Population",
      "description": "Members of the same species in a defined area with gene flow and demographic dynamics.",
      "broader": ["https://ontology.radical.world/level/community"],
      "narrower": ["https://ontology.radical.world/level/organism"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/community",
      "type": "LevelOfOrganization",
      "name": "Community",
      "alias": ["Biotic Community"],
      "description": "Interacting populations of different species in a shared locale; may include human communities.",
      "broader": ["https://ontology.radical.world/level/ecosystem"],
      "narrower": [
        "https://ontology.radical.world/level/population",
        "https://ontology.radical.world/level/city",
        "https://ontology.radical.world/level/organization"
      ],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/ecosystem",
      "type": "LevelOfOrganization",
      "name": "Ecosystem",
      "description": "Biotic communities plus abiotic environment linked by flows of matter, energy, and information.",
      "broader": ["https://ontology.radical.world/level/biome"],
      "narrower": [
        "https://ontology.radical.world/level/community",
        "https://ontology.radical.world/level/network"
      ],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/biome",
      "type": "LevelOfOrganization",
      "name": "Biome",
      "description": "Large ecological regions defined by climate and dominant vegetation (e.g., tundra, rainforest).",
      "broader": ["https://ontology.radical.world/level/biosphere"],
      "narrower": ["https://ontology.radical.world/level/ecosystem"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/biosphere",
      "type": "LevelOfOrganization",
      "name": "Biosphere",
      "alias": ["Planetary"],
      "description": "Global sum of ecosystems where life exists and cycles interact with Earth systems.",
      "broader": ["https://ontology.radical.world/level/stellar-system"],
      "narrower": [
        "https://ontology.radical.world/level/biome",
        "https://ontology.radical.world/level/region",
        "https://ontology.radical.world/level/network"
      ],
      "inScheme": "rad:LevelScheme"
    },

    {
      "@id": "https://ontology.radical.world/level/dyad",
      "type": "LevelOfOrganization",
      "name": "Dyad",
      "alias": ["Pair"],
      "description": "Two individuals coordinating together (e.g., caregiver–infant, mentor–mentee).",
      "broader": ["https://ontology.radical.world/level/household"],
      "narrower": [],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/household",
      "type": "LevelOfOrganization",
      "name": "Household",
      "alias": ["Home"],
      "description": "Co-residing unit sharing resources and daily coordination.",
      "broader": ["https://ontology.radical.world/level/group"],
      "narrower": ["https://ontology.radical.world/level/dyad"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/group",
      "type": "LevelOfOrganization",
      "name": "Group",
      "alias": ["Team","Crew"],
      "description": "Small set collaborating with shared intent and practices.",
      "broader": ["https://ontology.radical.world/level/organization"],
      "narrower": [
        "https://ontology.radical.world/level/household",
        "https://ontology.radical.world/level/network"
      ],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/organization",
      "type": "LevelOfOrganization",
      "name": "Organization",
      "alias": ["Org","Institution"],
      "description": "Structured entity coordinating multiple groups via roles, rules, and resources.",
      "broader": ["https://ontology.radical.world/level/city"],
      "narrower": [
        "https://ontology.radical.world/level/group",
        "https://ontology.radical.world/level/network"
      ],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/city",
      "type": "LevelOfOrganization",
      "name": "City",
      "alias": ["Town","Municipality","Metro"],
      "description": "Municipal-scale human settlement with shared infrastructure and governance.",
      "broader": ["https://ontology.radical.world/level/region"],
      "narrower": [
        "https://ontology.radical.world/level/organization",
        "https://ontology.radical.world/level/community",
        "https://ontology.radical.world/level/network"
      ],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/region",
      "type": "LevelOfOrganization",
      "name": "Region",
      "alias": ["County","State","Province","Bioregion"],
      "description": "Multi-city area sharing ecology, economy, and/or governance.",
      "broader": ["https://ontology.radical.world/level/biosphere"],
      "narrower": [
        "https://ontology.radical.world/level/city",
        "https://ontology.radical.world/level/network"
      ],
      "inScheme": "rad:LevelScheme"
    },

    {
      "@id": "https://ontology.radical.world/level/network",
      "type": "LevelOfOrganization",
      "name": "Network",
      "alias": ["Distributed Network"],
      "description": "Cross-boundary connections among actors or systems (often spans multiple other levels).",
      "broader": [
        "https://ontology.radical.world/level/region",
        "https://ontology.radical.world/level/ecosystem",
        "https://ontology.radical.world/level/city",
        "https://ontology.radical.world/level/organization",
        "https://ontology.radical.world/level/group",
        "https://ontology.radical.world/level/biosphere"
      ],
      "narrower": [],
      "inScheme": "rad:LevelScheme"
    },

    {
      "@id": "https://ontology.radical.world/level/stellar-system",
      "type": "LevelOfOrganization",
      "name": "Stellar System",
      "alias": ["Planetary System","Solar System"],
      "description": "A star and its orbiting bodies and fields.",
      "broader": ["https://ontology.radical.world/level/galactic"],
      "narrower": ["https://ontology.radical.world/level/biosphere"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/galactic",
      "type": "LevelOfOrganization",
      "name": "Galactic",
      "description": "A gravitationally bound system of stars, stellar remnants, gas, dust, and dark matter.",
      "broader": ["https://ontology.radical.world/level/intergalactic"],
      "narrower": ["https://ontology.radical.world/level/stellar-system"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/intergalactic",
      "type": "LevelOfOrganization",
      "name": "Intergalactic",
      "alias": ["Galaxy Cluster / Filament"],
      "description": "Matter and space between galaxies; groups, clusters, and filaments.",
      "broader": ["https://ontology.radical.world/level/universal"],
      "narrower": ["https://ontology.radical.world/level/galactic"],
      "inScheme": "rad:LevelScheme"
    },
    {
      "@id": "https://ontology.radical.world/level/universal",
      "type": "LevelOfOrganization",
      "name": "Universal",
      "alias": ["Cosmic","Universe"],
      "description": "The totality of spacetime, matter, energy, and information.",
      "broader": [],
      "narrower": ["https://ontology.radical.world/level/intergalactic"],
      "inScheme": "rad:LevelScheme"
    }
  ]
}
